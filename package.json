{
  "name": "triny",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "del-cli dist && del-cli build",
    "compile": "tsc",
    "bundle": "set NODE_ENV=development&& rollup -c rollup.config.js",
    "bundle:prod": "set NODE_ENV=production&& rollup -c rollup.config.js",
    "watch": "set NODE_ENV=development&& rollup -c rollup.config.js -w",
    "assets": "copyfiles ./assets/**/*.* -f dist/",
    "build": "npm run compile && npm run assets && npm run watch",
    "build:prod": "npm run compile && npm run bundle:prod && npm run assets",
    "start": "light-server -s ./dist -o"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "effector": "^21.8.12",
    "effector-react": "^21.3.2",
    "firebase": "^8.6.8",
    "light-server": "^2.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/react-dom": "^17.0.8",
    "autoprefixer": "^10.2.6",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.0",
    "rollup": "^2.52.3",
    "rollup-plugin-conditional": "^3.1.2",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.3.4"
  }
}
