    "eol-converter-cli": "^1.0.8",
    "rollup": "^2.52.3",
    "rollup-plugin-conditional": "^3.1.2",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-node-builtins": "^2.1.2",
