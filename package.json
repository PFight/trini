{
  "name": "triny",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "del-cli docs",
    "bundle": "set NODE_ENV=development&& rollup -c rollup.config.js",
    "bundle:prod": "set NODE_ENV=production&& rollup -c rollup.config.js",
    "watch": "set NODE_ENV=development&& rollup -c rollup.config.js -w",
    "assets": "copyfiles ./assets/**/*.* -f docs/",
    "makeIndex": "barrelsby -c generate-index-config.json && eolConverter \"./src/**/index.ts\"",
    "build": "npm run makeIndex && npm run assets && npm run watch",
    "build:prod": "npm run makeIndex && npm run bundle:prod && npm run assets",
    "start": "light-server -s ./docs -o",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.15.0",
