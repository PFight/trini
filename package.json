    "effector-react": "^21.3.2",
    "firebase": "^8.6.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "light-server": "^2.9.1",
